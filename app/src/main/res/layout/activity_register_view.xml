<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:id="@+id/rf671cq4zqt"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#292E32">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/r9qzrhxe6kiu"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:paddingVertical="6dp"
                android:paddingHorizontal="1dp"
                android:layout_marginBottom="16dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/rifqmy4x3kb"
                    android:textColor="#FFFFFF"
                    android:textSize="24sp"
                    android:textStyle="bold"
                    android:layout_marginBottom="8dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Talk2Sign"
                    />
                <TextView
                    android:id="@+id/rsf2g2w4uccg"
                    android:textColor="#BBBBBB"
                    android:textSize="15sp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="Zeinu itzultzaile moderno eta automatikoa"
                    />
            </LinearLayout>
            <TextView
                android:id="@+id/r41phgab5r0m"
                android:textColor="#FFFFFF"
                android:textSize="25sp"
                android:textStyle="bold"
                android:layout_marginBottom="15dp"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="Erregistroa"
                />
            <LinearLayout
                android:id="@+id/rifbkbv1n4u"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingHorizontal="16dp"
                android:layout_marginBottom="11dp"
                android:orientation="vertical">
                <EditText
                    android:inputType="text"
                    android:hint="Izena"
                    android:id="@+id/ravf8utv1fer"
                    android:textColor="#919191"
                    android:textSize="16sp"
                    android:layout_marginBottom="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/background_txtviews"
                    android:paddingVertical="17dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="32dp"
                    />
                <EditText
                    android:inputType="text"
                    android:hint="Abizenak"
                    android:id="@+id/r4h143xke9d3"
                    android:textColor="#919191"
                    android:textSize="16sp"
                    android:layout_marginBottom="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/background_txtviews"
                    android:paddingVertical="17dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="32dp"
                    />
                <EditText
                    android:inputType="text"
                    android:hint="E-posta"
                    android:id="@+id/rm0fwpvbpgei"
                    android:textColor="#919191"
                    android:textSize="16sp"
                    android:layout_marginBottom="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/background_txtviews"
                    android:paddingVertical="17dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="32dp"
                    />
                <EditText
                    android:inputType="text"
                    android:hint="Pasahitza"
                    android:id="@+id/rl2vxg3qph27"
                    android:textColor="#919191"
                    android:textSize="16sp"
                    android:layout_marginBottom="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/background_txtviews"
                    android:paddingVertical="17dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="32dp"
                    />
                <EditText
                    android:inputType="text"
                    android:hint="Jaiotze-data"
                    android:id="@+id/rszo19fsb6i"
                    android:textColor="#919191"
                    android:textSize="16sp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/background_txtviews"
                    android:paddingVertical="17dp"
                    android:paddingLeft="16dp"
                    android:paddingRight="32dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rdtt80qhh55j"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/background_txtviews"
                android:paddingVertical="17dp"
                android:paddingHorizontal="16dp"
                android:layout_marginBottom="11dp"
                android:layout_marginHorizontal="16dp"
                android:clickable="true"
                android:focusable="true"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/r7casivw70ng"
                    android:textColor="#919191"
                    android:textSize="16sp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="Pasahitza"
                    />
                <ImageView
                    android:id="@+id/r28rre0z93a8"
                    android:layout_width="20dp"
                    android:layout_height="18dp"
                    android:layout_marginVertical="1dp"
                    android:src="@mipmap/ic_launcher"
                    android:scaleType="fitXY"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rwyztia3izg9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/background_txtviews"
                android:paddingVertical="17dp"
                android:paddingHorizontal="16dp"
                android:layout_marginBottom="51dp"
                android:layout_marginHorizontal="16dp"
                android:clickable="true"
                android:focusable="true"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/rfdr1xk76o7b"
                    android:textColor="#919191"
                    android:textSize="16sp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="Pasahitza errepikatu"
                    />
                <ImageView
                    android:id="@+id/rgxj2cqj3mx4"
                    android:layout_width="20dp"
                    android:layout_height="18dp"
                    android:layout_marginVertical="1dp"
                    android:src="@mipmap/ic_launcher"
                    android:scaleType="fitXY"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rqthfnm8gh3d"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:background="@drawable/buttons_color"
                android:paddingVertical="17dp"
                android:layout_marginBottom="50dp"
                android:layout_marginHorizontal="16dp"
                android:clickable="true"
                android:focusable="true"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/r78fw3i0832q"
                    android:textColor="#FFFFFF"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Erregistratu"
                    />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>